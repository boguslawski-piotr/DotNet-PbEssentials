<?xml version="1.0" encoding="UTF-8"?>
<pbXForms:BaseContentView
	xmlns = "http://xamarin.com/schemas/2014/forms"
	xmlns:x = "http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:pbXForms = "clr-namespace:pbXForms;assembly=pbXForms"
	x:Class = "pbXForms.ContentView">

	<AbsoluteLayout
		x:Name = "_Layout">

		<pbXForms:ContentViewLayout
			x:Name = "_ContentLayout">

			<pbXForms:AppBarLayout
				Grid.Row = "0"
				Grid.Column = "0"
				x:Name = "_AppBarRow">

			</pbXForms:AppBarLayout>

			<pbXForms:ContentViewViewLayout
				Grid.Row = "1"
				Grid.Column = "0"
				x:Name = "_ViewRow">

			</pbXForms:ContentViewViewLayout>

			<pbXForms:ToolBarLayout
				Grid.Row = "2"
				Grid.Column = "0"
				x:Name = "_ToolBarRow">

			</pbXForms:ToolBarLayout>

		</pbXForms:ContentViewLayout>

	</AbsoluteLayout>

</pbXForms:BaseContentView>
